<%= form_with model: [:admin, @test, @question], local: true do |form| %>
  <h1>
    <%= question_header(question: @question) %>
  </h1>
  <%= render 'shared/errors', resource: @question %>
  <p>
    <%= form.label :body %><br>
    <%= form.text_field :body, value: @question.body %><br>
  </p>
  <p><%= form.submit %></p>
<% end %>
