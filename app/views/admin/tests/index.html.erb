<h1>All tests</h1>
<p>
  <%= link_to 'New test', new_admin_test_path %>
</p>

<table>
  <tr>
    <th>Title</th>
    <th>Level</th>
    <th>Questions number</th>
    <th colspan="3">Actions</th>
    <%= render @test %>
</table>

<h3>Gists</h3>
<table>
  <tr>
    <th>Question</th>
    <th>Gist URL</th>
    <th>User email</th>
    <%= render @gist %>
</table>